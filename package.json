{
  "name": "@thaitype/minimal-typescript-node-esm-starter",
  "version": "0.0.0",
  "description": "minimal-typescript-node-esm-starter",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.js",
      "default": "./dist/main.cjs"
    }
  },
  "scripts": {
    "start": "tsx src/main.ts",
    "dev": "tsx watch src/main.ts",
    "test:coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ci": "vitest run",
    "build": "esbuild src/main.ts --bundle --platform=node --target=node16 --format=cjs --outfile=dist/main.cjs --minify",
    "format": "prettier -w src",
    "lint": "tsc --noEmit && eslint ./src && prettier -c src",
    "lint:fix": "eslint --fix ./src && prettier -w src",
    "azurite": "pnpx azurite --silent --location ./.azurite --debug ./.azurite/debug.log"
  },
  "keywords": [],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "devDependencies": {
    "@azure/core-paging": "^1.5.0",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@vitest/coverage-v8": "^1.1.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@thaitype/record-parser": "^1.0.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.0"
  }
}